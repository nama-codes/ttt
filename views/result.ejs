<% include header.ejs %>
<div class="page-header">
  <h1>Top <%=number%> Words and their Frequency</h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <table class="table table-hover" align="center">
      <thead>
        <tr>
          <th>#</th>
          <th>Word</th>
          <th>Frequency</th>
        </tr>
      </thead>
      <tbody>
        <%let count = 0;%>
        <%result.forEach((arr) => {%>
        <tr>
          <td><%=++count%></td>
          <td><%=arr[0]%></td>
          <td><%=arr[1]%></td>
        </tr>
        <%})%>
      </tbody>
    </table>
  </div>
</div>
<% include footer.ejs %>
